<?xml version="1.0" encoding="UTF-8"?>
<taglib	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://java.sun.com/xml/ns/javaee"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_3_0.xsd"
		version="3.0">

	<description>HIPPO Custom Tag Library</description>
	<tlib-version>1.0</tlib-version>
	<short-name>hippo</short-name>
	<uri>http://hippo.nhncorp.com/hippo</uri>

	<function>
		<description></description>
		<name>bytesToString</name>
		<function-class>com.nhn.hippo.web.utils.AnnotationUtils</function-class>
		<function-signature>java.lang.String bytesToString(int, byte[])</function-signature>
	</function>
	
	<function>
		<description></description>
		<name>longToDateStr</name>
		<function-class>com.nhn.hippo.web.utils.AnnotationUtils</function-class>
		<function-signature>java.lang.String longToDateStr(long)</function-signature>
	</function>
	
	<function>
		<description></description>
		<name>longLongToUUID</name>
		<function-class>com.nhn.hippo.web.utils.AnnotationUtils</function-class>
		<function-signature>java.lang.String longLongToUUID(long, long)</function-signature>
	</function>
</taglib>